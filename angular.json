{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "muri": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/muri",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/intl-tel-input/build/css/intlTelInput.css",
              "src/styles.scss",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/animate.min.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/slick.css",
              "src/assets/css/custom-animation.css",
              "src/assets/css/default.css",
              "src/assets/css/rtl.css"

            ],
            "scripts": [
              "src/assets/js/modernizr-3.6.0.min.js",
              "src/assets/js/jquery-1.12.4.min.js",
              "src/assets/js/popper.min.js",
              "src/assets/js/wow.js",
              "src/assets/js/jquery.counterup.min.js",
              "src/assets/js/waypoints.min.js",
              "src/assets/js/TweenMax.min.js",
              "src/assets/js/slick.min.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/main.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3.5mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "1mb",
                  "maximumError": "1mb"
                }
              ],
              "outputHashing": "all",
              "sourceMap": false,
              "optimization": true,
              "extractLicenses": true
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "muri:build:production"
            },
            "development": {
              "buildTarget": "muri:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/animate.min.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/slick.css",
              "src/assets/css/custom-animation.css",
              "src/assets/css/default.css",
              "src/assets/css/rtl.css"
            ],
            "scripts": [
              "src/assets/js/modernizr-3.6.0.min.js",
              "src/assets/js/jquery-1.12.4.min.js",
              "src/assets/js/popper.min.js",
              "src/assets/js/wow.js",
              "src/assets/js/jquery.counterup.min.js",
              "src/assets/js/waypoints.min.js",
              "src/assets/js/TweenMax.min.js",
              "src/assets/js/slick.min.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/main.js"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "2aeae1b5-1507-4f19-b066-0268411ece78"
  }
}
